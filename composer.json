    "minimum-stability": "dev",
    "require": {
		"php": ">=5.4",
		"o3com/crawler": "master@dev",
		"symfony/console": "~2.5.0",
		"symfony/finder": "~2.5.0",
		"symfony/yaml": "~2.5.0",
		"symfony/dependency-injection": "~2.5.0",
		"symfony/config": "~2.5.0",
		"symfony/event-dispatcher": "~2.5.0",
		"symfony/stopwatch": "~2.5.0",
		"symfony/monolog-bridge": "~2.5.0",
		
		"symfony/http-foundation": "~2.5.0",
		"fabpot/goutte": "~2.0.0",

		"guzzlehttp/guzzle": "~5.1",
		"guzzlehttp/log-subscriber": "~1.0",
		"guzzlehttp/retry-subscriber": "~2.0",

		"monolog/monolog": "@stable"
	},
    "autoload": {
        "psr-0": { "": "src/" }
